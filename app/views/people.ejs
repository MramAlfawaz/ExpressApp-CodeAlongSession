<h1>People Page</h1>
<table>
    <tr>
        <th> First Name </th>
        <th> Last Name </th>
        <th> ID </th>

    </tr>
<% data.forEach(function(data, index) { %>
    <tr>
        <td><%= data.firstName %></td>
        <td><%= data.lastName %></td>
        <td><%= data.id %></td>

        <td> <button type="update" name="update" class="Edit" onclick="crudApp.Update(this)">update</button> </td>
        <td> <button type="delete" name="delete" onclick="crudApp.Delete(this)">delete</button> </td>

    </tr>
    <% }) %>
</table>